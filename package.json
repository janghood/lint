{
  "name": "@janghood/lint",
  "version": "0.0.2",
  "description": "极客江湖lint库",
  "author": "阿怪 <higuaifan@higuaifan.com>",
  "main": "index.js",
  "type": "module",
  "files": [
    "config",
    "lib",
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "run": "tsx lib/bin/jhlint.ts",
    "run-install": "tsx lib/bin/jhlint.ts install",
    "run-config": "tsx lib/bin/jhlint.ts --config janghood.dev.config.ts",
    "run-help": "tsx lib/bin/jhlint.ts --help",
    "dist-dev": "npm run build && node dist/bin/jhlint.mjs install",
    "rm-dist": "rm -rf dist",
    "build": "npm run rm-dist && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript && tsx ./cli/bin",
    "test": "vitest -u",
    "build:eslint": "eslint --config dist/eslint.cjs .",
    "lint": "jhlint",
    "oxlint": "oxlint"
  },
  "bin": {
    "jhlint": "dist/bin/jhlint.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janghood/lint.git"
  },
  "keywords": [
    "janghood",
    "lint",
    "prettier",
    "husky",
    "eslint"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@stylistic/eslint-plugin": "^1.5.4",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-oxlint": "^0.2.3",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^9.0.7",
    "oxlint": "^0.2.6"
  },
  "devDependencies": {
    "@commitlint/types": "^18.6.0",
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/js": "^8.56.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@stylistic/eslint-plugin": "^1.5.4",
    "@types/eslint": "^8.56.2",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.11.15",
    "@types/text-table": "^0.2.5",
    "@typescript-eslint/utils": "^6.21.0",
    "@janghood/eslint-plugin": "workspace:*",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@janghood/config": "0.0.1-3",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-oxlint": "^0.2.3",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^9.0.7",
    "oxlint": "^0.2.6",
    "strip-ansi": "^7.1.0",
    "text-table": "^0.2.0"
  }
}
