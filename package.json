{
  "name": "@janghood/lint",
  "version": "0.0.1-5",
  "description": "极客江湖lint库",
  "author": "阿怪 <higuaifan@higuaifan.com>",
  "main": "index.js",
  "type": "module",
  "files": [
    "config",
    "lib",
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "run": "tsx lib/bin/jhlint.ts",
    "run-install": "tsx lib/bin/jhlint.ts install",
    "dist-dev": "npm run build && node dist/bin/jhlint.mjs install",
    "rm-dist": "rm -rf dist",
    "build": "npm run rm-dist && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "eslint": "eslint --config dist/eslint.js ."
  },
  "bin": {
    "jhlint": "dist/bin/jhlint.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janghood/lint.git"
  },
  "keywords": [
    "janghood",
    "lint",
    "prettier",
    "husky",
    "eslint"
  ],
  "license": "MIT",
  "peerDependencies": {
    "chalk": "^5.1.2",
    "eslint": "^8.27.0",
    "husky": "^8.0.2"
  },
  "devDependencies": {
    "@commitlint/types": "^17.0.0",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.9",
    "@types/strip-ansi": "^5.2.1",
    "@types/text-table": "^0.2.2",
    "rollup": "^3.3.0",
    "tslib": "^2.4.1",
    "tsx": "^3.12.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@janghood/config": "0.0.1-1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "chalk": "^5.1.2",
    "eslint": "^8.27.0",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.2",
    "strip-ansi": "^7.0.1",
    "text-table": "^0.2.0"
  }
}
